filetype indent plugin on
color molokai
set showcmd
syntax on
set hidden
set hlsearch
set ignorecase
set smartcase
set number
set shiftwidth=4
set softtabstop=4
set expandtab
nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
let g:molokai_original = 1
let g:rehash256 = 1
